{
	"compilerOptions": {
		"strict": true,
		"moduleResolution": "node",
		"declaration": true,
		"sourceMap": true,
		"removeComments": false,
		"alwaysStrict": true,
		"pretty": true,
		"outDir": "dist",
		"incremental": true,
		"noEmitHelpers": true,
		"importHelpers": true,
		"skipLibCheck": true
	}
}
