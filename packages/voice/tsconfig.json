{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "lib": [
            "esnext",
            "esnext.array",
            "esnext.asynciterable",
            "esnext.intl",
            "esnext.symbol",
        ],
        "sourceMap": true,
        "inlineSources": true,
        "outDir": "dist",
        "incremental": true,
        "noEmitHelpers": true,
        "importHelpers": true,
        "skipLibCheck": true
    }
}
